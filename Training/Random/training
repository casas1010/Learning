function palindrome(str) {
    var str=str.replace(/\s+/g, '');
    str = str.replace(/\s+/g, '+');
 str  = str.replace(/[^a-zA-Z0-9+]/g, "");
    var len=str.length;
    var i,n;
    var test="";
    for (i=0 n=len; n>0 ; n--){
      test+= str[len];
    }
    return test
    }

palindrome("car");

console.log('hello world');


sum(2,3); 